def very(data, n):
    print(f'Макс. {data[0][n]}: ' + max(data[1:], key=lambda x: x[n])[0])
    print(f'Мин. {data[0][n]}: ' + min(data[1:], key=lambda x: x[n])[0])
very(data, 1)
very(data, 2)
very(data, 3)
